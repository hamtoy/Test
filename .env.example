# ===========================================
# 필수 설정
# ===========================================
GEMINI_API_KEY=your_api_key_here

# ===========================================
# Gemini API 설정 (선택)
# ===========================================
GEMINI_MODEL_NAME=gemini-3-pro-preview
GEMINI_MAX_OUTPUT_TOKENS=8192
GEMINI_TIMEOUT=120
GEMINI_MAX_CONCURRENCY=5
GEMINI_TEMPERATURE=0.2
GEMINI_CACHE_SIZE=50
GEMINI_CACHE_TTL_MINUTES=360

# ===========================================
# 캐싱 설정 (선택)
# ===========================================
# CACHE_STATS_FILE=cache_stats.jsonl
# CACHE_STATS_MAX_ENTRIES=100
# LOCAL_CACHE_DIR=.cache

# Gemini API 제약 - 변경 불필요
# GEMINI_CACHE_MIN_TOKENS=2048

# ===========================================
# 로깅 설정 (선택)
# ===========================================
LOG_LEVEL=INFO

# ===========================================
# 예산 설정 (선택)
# ===========================================
# BUDGET_LIMIT_USD=10.0
# BUDGET_WARNING_THRESHOLD=0.8

# ===========================================
# Rate Limiting (선택)
# ===========================================
# RATE_LIMIT_RPM=60
# MAX_CONCURRENT_REQUESTS=5

# ===========================================
# Neo4j 설정 (RAG 사용 시)
# ===========================================
# ENABLE_RAG=false
# NEO4J_URI=neo4j+s://your-instance.databases.neo4j.io
# NEO4J_USER=neo4j
# NEO4J_PASSWORD=your_password_here

# ===========================================
# Notion 설정 (그래프 빌더용)
# ===========================================
# NOTION_API_KEY=your_notion_integration_key
# NOTION_PAGE_IDS=page_id_1,page_id_2
# NOTION_DATABASE_ID=your_database_id

# ===========================================
# 기능 플래그 (선택)
# ===========================================
# ENABLE_LATS=false
# ENABLE_DATA2NEO=false
# DATA2NEO_BATCH_SIZE=100
# DATA2NEO_CONFIDENCE_THRESHOLD=0.7

# ===========================================
# Redis 설정 (워커 사용 시)
# ===========================================
# REDIS_URL=redis://localhost:6379
