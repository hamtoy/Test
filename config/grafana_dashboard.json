{
  "dashboard": {
    "title": "QA System Performance Dashboard",
    "tags": ["qa", "gemini", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Latency (p99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(api_request_latency_seconds_bucket[5m]))",
            "legendFormat": "{{endpoint}} - p99"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_requests_total[1m])",
            "legendFormat": "{{endpoint}} - {{method}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Token Usage (Last Hour)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(token_usage_total[1h])",
            "legendFormat": "{{model}} - {{type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "API Cost (USD/hour)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_cost_usd_total[1h]) * 3600",
            "legendFormat": "{{model}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m]))",
            "legendFormat": "{{cache_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Error Rate (%)",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(api_requests_total{status=~\"5..\"}[5m]) / rate(api_requests_total[5m]) * 100",
            "legendFormat": "Error %"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}
      },
      {
        "id": 7,
        "title": "Total Requests (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(api_requests_total[24h])",
            "legendFormat": "Total"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16}
      }
    ],
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
