# Gemini API Key
GEMINI_API_KEY=your_api_key_here
GEMINI_MODEL_NAME=gemini-3-pro-preview

# Neo4j Configuration (for QA graph system)
NEO4J_URI=neo4j+s://your-instance.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password_here

# Notion Configuration (for graph builder)
NOTION_API_KEY=your_notion_integration_key
NOTION_PAGE_IDS=page_id_1,page_id_2
NOTION_DATABASE_ID=your_database_id

# Optional: OpenAI for vector search
# OPENAI_API_KEY=your_openai_key

# Optional: Gemini model override
# GEMINI_MODEL_NAME=gemini-3-pro-preview

# Optional: Cache statistics
# CACHE_STATS_FILE=cache_stats.jsonl
# CACHE_STATS_MAX_ENTRIES=1000
